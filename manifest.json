{   
    "name": "Reminders",
    "description" : "Chrome Extension to Remind",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
        "description": "Opens hello.html"
        }
    },
    "background": {
        "scripts": ["background.js"]
    }
}
